def repo_url = 'https://github.com/GabrielWal/docker-lessons.git'
def repo_branch = 'master'
node {
    stage("Get project"){
        git branch: repo_branch, url: repo_url
    }

    stage("Build project"){
        sh "cd part-2/src && \
        cmake CMakeLists.txt && \
        make"
    }

    stage("Test project"){
        sh "cmake ../ && cmake --build ."
    }
}